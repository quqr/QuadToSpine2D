<sukiUi:SukiWindow
    xmlns="https://github.com/avaloniaui"
    xmlns:sukiUi="clr-namespace:SukiUI.Controls;assembly=SukiUI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:fluent="clr-namespace:FluentIcons.Avalonia.Fluent.Markup;assembly=FluentIcons.Avalonia.Fluent"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:vm="clr-namespace:QTSAvalonia.ViewModels.Pages"
    x:DataType="vm:RootViewModel"
    xmlns:suki="https://github.com/kikipoulet/SukiUI"
    xmlns:pages="clr-namespace:QTSAvalonia.Views.Pages"
    xmlns:util="clr-namespace:QTSAvalonia.Helper"
    mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="500"
    x:Class="QTSAvalonia.Views.Pages.RootView"
    Title="QuadToSpine2D"
    MinHeight="750" MinWidth="1200">
    <suki:SukiSideMenu>
        <suki:SukiSideMenu.Items>
            <suki:SukiSideMenuItem IsContentMovable="False" Header="Home"
                                   Icon="{fluent:FluentIcon Icon=TasksApp,IconVariant=Regular}">
                <suki:SukiSideMenuItem.PageContent>
                    <pages:Converter />
                </suki:SukiSideMenuItem.PageContent>
            </suki:SukiSideMenuItem>
            <suki:SukiSideMenuItem IsContentMovable="False" Header="Previewer"
                                   Icon="{fluent:FluentIcon Icon=Play,IconVariant=Regular}">
                <suki:SukiSideMenuItem.PageContent>
                    <pages:Player />
                </suki:SukiSideMenuItem.PageContent>
            </suki:SukiSideMenuItem>
            <suki:SukiSideMenuItem IsContentMovable="False" Header="DataBase"
                                   Icon="{fluent:FluentIcon Icon=Database,IconVariant=Regular}">
                <suki:SukiSideMenuItem.PageContent>
                    <pages:DataBase />
                </suki:SukiSideMenuItem.PageContent>
            </suki:SukiSideMenuItem>
            <suki:SukiSideMenuItem IsContentMovable="False" Header="Setting"
                                   Icon="{fluent:FluentIcon Icon=Settings,IconVariant=Regular}">
                <suki:SukiSideMenuItem.PageContent>
                    <pages:Settings />
                </suki:SukiSideMenuItem.PageContent>
            </suki:SukiSideMenuItem>
        </suki:SukiSideMenu.Items>
    </suki:SukiSideMenu>
    <suki:SukiWindow.Hosts>
        <suki:SukiToastHost Manager="{x:Static util:ToastHelper.ToastManager}" />
    </suki:SukiWindow.Hosts>
</sukiUi:SukiWindow>